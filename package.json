{
  "name": "fileinator",
  "displayName": "Fileinator",
  "description": "Scans folders and displays file contents or tree structure in a new tab",
  "version": "0.0.1",
  "publisher": "shinazbinshajahan",
  "engines": {
      "vscode": "^1.85.0"
  },
  "categories": [
      "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
      "commands": [
          {
              "command": "fileinator.generateCompleteData",
              "title": "Fileinator - Generate Complete Data"
          }
      ],
      "menus": {
          "explorer/context": [
              {
                  "when": "resourceDirname",
                  "command": "fileinator.generateCompleteData",
                  "group": "navigation"
              }
          ]
      }
  },
  "scripts": {
      "lint": "eslint .",
      "pretest": "npm run lint",
      "test": "node ./test/runTest.js"
  },
  "devDependencies": {
      "@types/vscode": "^1.85.0",
      "@types/mocha": "^10.0.6",
      "@types/node": "18.x",
      "eslint": "^8.56.0",
      "mocha": "^10.2.0",
      "@vscode/test-electron": "^2.3.9"
  }
}